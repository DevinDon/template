{
  "name": "template.typescript",
  "version": "0.0.0",
  "description": "TypeScript Template.",
  "main": "dist/bundle.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "MODE=DEV ./scripts/build",
    "build:prod": "MODE=PROD ./scripts/build",
    "clean": "./scripts/clean",
    "debug": "./scripts/debug",
    "postinstall": "npm run init",
    "init": "./scripts/init",
    "lint": "./scripts/lint",
    "pack": "npm run clean && npm run build && npm run push",
    "push": "./scripts/push",
    "service": "./scripts/service",
    "start": "npm run start:dev",
    "start:dev": "MODE=DEV ./scripts/start",
    "start:prod": "MODE=PROD ./scripts/start",
    "test": "./scripts/test"
  },
  "keywords": [
    "typescript",
    "project"
  ],
  "author": "IInfinity <i.inf@outlook.com> (https://blog.don.red/)",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/jasmine": "3.7.8",
    "@types/node": "16.0.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "cross-env": "7.0.3",
    "esbuild": "0.12.15",
    "eslint": "7.30.0",
    "jasmine": "3.8.0",
    "jasmine-console-reporter": "3.1.0",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/template.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/template/issues"
  },
  "homepage": "https://github.com/DevinDon/template/tree/typescript"
}
