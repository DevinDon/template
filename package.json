{
  "name": "template.rester",
  "version": "0.13.1",
  "description": "Rester Template.",
  "main": "dist/main.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "./script/build.sh",
    "clean": "./script/clean.sh",
    "debug": "cross-env NODE_ENV=DEV node -r ts-node/register src/main/main.ts",
    "demo": "cross-env NODE_ENV=DEV node -r ts-node/register src/demo/main.ts",
    "init": "./script/init-env.sh",
    "lint": "eslint . --fix",
    "pack": "npm run build && npm run push",
    "push": "./script/push.sh",
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=DEV node -r ts-node/register src/main/main.ts",
    "start:prod": "npm run build && cd dist && cross-env NODE_ENV=PROD node main.js",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=src/test/jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register",
    "watch": "tsc -w"
  },
  "keywords": [
    "typescript",
    "rester",
    "backend"
  ],
  "author": "IInfinity <I.INF@Outlook.com> (https://blog.don.red/)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@rester/core": "^0.13.19",
    "mongodb": "^3.6.4"
  },
  "devDependencies": {
    "@types/jasmine": "3.6.6",
    "@types/mongodb": "^3.6.9",
    "@types/node": "14.14.34",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.22.0",
    "jasmine": "3.6.4",
    "jasmine-console-reporter": "3.1.0",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack": "5.25.0",
    "webpack-cli": "4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/template.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/template/issues"
  },
  "homepage": "https://github.com/DevinDon/template/tree/typescript"
}
