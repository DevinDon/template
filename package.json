{
  "name": "template.rester",
  "version": "0.15.0",
  "description": "Rester Template.",
  "main": "dist/bundle.js",
  "directories": {
    "doc": "docs",
    "example": "demo"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "MODE=DEV ./scripts/build",
    "build:prod": "MODE=PROD ./scripts/build",
    "clean": "./scripts/clean",
    "debug": "./scripts/debug",
    "postinstall": "npm run init",
    "init": "./scripts/init",
    "lint": "./scripts/lint",
    "pack": "npm run clean && npm run build && npm run push",
    "push": "./scripts/push",
    "service": "./scripts/service",
    "start": "npm run start:dev",
    "start:dev": "MODE=DEV ./scripts/start",
    "start:prod": "MODE=PROD ./scripts/start",
    "test": "./scripts/test"
  },
  "keywords": [
    "typescript",
    "rester",
    "backend"
  ],
  "author": "IInfinity <i.inf@outlook.com> (https://blog.don.red/)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@rester/core": "0.15.21",
    "@rester/logger": "0.2.2",
    "@rester/orm": "0.1.9",
    "class-validator": "0.13.1",
    "mongodb": "3.6.6",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/jasmine": "3.6.10",
    "@types/mongodb": "3.6.12",
    "@types/node": "15.0.1",
    "@types/node-fetch": "2.5.10",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "esbuild": "0.11.16",
    "eslint": "7.25.0",
    "jasmine": "3.7.0",
    "jasmine-console-reporter": "3.1.0",
    "ts-loader": "9.1.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/template.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/template/issues"
  },
  "homepage": "https://github.com/DevinDon/template/tree/rester"
}
