{
  "name": "template.typescript",
  "version": "0.0.0",
  "description": "TypeScript Template.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "bash script/build.sh",
    "clean": "bash script/clean.sh",
    "lint": "eslint .",
    "pack": "npm run build && npm run push",
    "push": "bash script/push.sh",
    "prepublishOnly": "npm run clean && tsc",
    "start": "npm run start:dev",
    "start:debug": "node --inspect-brk -r ts-node/register src/main/index.ts",
    "start:demo": "cross-env MODE=DEV node -r ts-node/register src/demo/index.ts",
    "start:dev": "cross-env MODE=DEV node -r ts-node/register src/main/index.ts",
    "start:prod": "npm run build && cross-env MODE=PROD node dist",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=src/test/jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register",
    "watch": "tsc -w"
  },
  "keywords": [
    "typescript",
    "project"
  ],
  "author": "IInfinity <I.INF@Outlook.com> (https://blog.don.red/)",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/jasmine": "3.6.7",
    "@types/node": "14.14.35",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "8.1.0",
    "cross-env": "7.0.3",
    "eslint": "7.22.0",
    "jasmine": "3.7.0",
    "jasmine-console-reporter": "3.1.0",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack": "5.27.2",
    "webpack-cli": "4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/template.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/template/issues"
  },
  "homepage": "https://github.com/DevinDon/template/tree/typescript"
}
